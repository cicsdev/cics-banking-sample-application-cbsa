  "license": "Apache-2.0",
  "scripts": {
    "build": "react-scripts build",
    "ci-check": "yarn format:diff",
    "clean": "yarn cache clean && yarn install",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{js,md,scss}\"",
    "format:diff": "prettier --list-different \"**/*.{js,md,scss}\"",
    "format:staged": "prettier --write",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@carbon/react": "1.61.0",
    "@svgr/plugin-svgo": "6.0.0",
    "axios": "^1.4.0",
    "build": "^0.1.4",
    "css-select": "5.1.0",
    "dotenv": "^16.3.1",
    "ejs": "3.1.10",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.7",
    "lodash": "4.17.21",
    "nth-check": "2.0.1",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dotenv": "^0.1.3",
    "react-native-dotenv": "^3.4.9",
    "react-router-dom": "5.1.0",
    "react-scripts": "5.0.1",
    "sass": "1.51.0",
    "semver": "7.5.2",
    "svgo": "3.0.2",
    "tough-cookie": "4.1.3",
    "webpack": "5.76.0",
    "webpack-dev-middleware": "5.3.4",
    "webpack-dev-server": "4.15.2",
    "word-wrap": "1.2.4"
  },
  "resolutions": {
    "js-yaml": "3.13.1",
    "uglify-js": "2.6.0",
    "postcss": "8.4.31",
    "ws": "8.17.1",
    "braces": "3.0.3",
    "micromatch": "4.0.6",
    "async": "3.2.5",
    "ejs": "3.1.10",
    "nth-check": "2.0.1",
    "semver": "7.5.2",
    "word-wrap": "1.2.4",
    "webpack-dev-middleware": "5.3.4",
    "follow-redirects": "1.15.6",
    "express": "4.19.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.0-placeholder-for-preset-env.2",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "husky": "^8.0.3",
    "lint-staged": "13.2.3",
    "prettier": "1.17.0",
    "wait-for-expect": "3.0.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "yarn format:staged",
      "git add"
    ],
    "*.{scss,css}": [
      "yarn format:staged",
      "git add"
    ],
    "*.md": [
      "yarn format:staged",
      "git add"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
